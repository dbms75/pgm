         2.2 ARITHMETIC ,LOGICAL,COMPARISON OPERATOR
SQL> create table emp5(eno number(5),ename varchar(10),salary number(10));
Table created.
SQL> desc emp5;
 Name					   Null?    Type
 ----------------------------------------- -------- ----------------------------
 ENO						    NUMBER(5)
 ENAME						    VARCHAR2(10)
 SALARY 					    NUMBER(10)
SQL> insert into emp5 values(&eno,'&ename',&salary);            
Enter value for eno: 2201
Enter value for ename: ragu
Enter value for salary: 500000
old   1: insert into emp5 values(&eno,'&ename',&salary)
new   1: insert into emp5 values(2201,'ragu',500000)
1 row created.
SQL> /
Enter value for eno: 2202
Enter value for ename: thasu
Enter value for salary: 700000
old   1: insert into emp5 values(&eno,'&ename',&salary)
new   1: insert into emp5 values(2202,'thasu',700000)
1 row created.
SQL> /
Enter value for eno: 2203
Enter value for ename: jack
Enter value for salary: 400000 
old   1: insert into emp5 values(&eno,'&ename',&salary)
new   1: insert into emp5 values(2203,'jack',400000)
1 row created.
SQL> / 
Enter value for eno: 2203
Enter value for ename: shajee
Enter value for salary: 600000
old   1: insert into emp5 values(&eno,'&ename',&salary)
new   1: insert into emp5 values(2203,'shajee',600000)
1 row created.
SQL> select * from emp5;
       ENO ENAME	  SALARY
---------- ---------- ----------
      2201 ragu 	  500000
      2202 thasu	  700000
      2203 jack 	  400000
      2203 shajee	  600000
SQL> select eno,ename,salary+100 as "bonus" from emp5;
       ENO ENAME	   bonus
---------- ---------- ----------
      2201 ragu 	  500100
      2202 thasu	  700100
      2203 jack 	  400100
      2203 shajee	  600100
SQL> select eno,ename,salary-100 as "decrement" from emp5;
       ENO ENAME       decrement
---------- ---------- ----------
      2201 ragu 	  499900
      2202 thasu	  699900
      2203 jack 	  399900
      2203 shajee	  599900
SQL> select eno,ename,salary * 100 as "increment" from emp5;
       ENO ENAME       increment
---------- ---------- ----------
      2201 ragu 	50000000
      2202 thasu	70000000
      2203 jack 	40000000
      2203 shajee	60000000
SQL> select eno,ename,salary / 100 as "incentive" from emp5;
       ENO ENAME       incentive
---------- ---------- ----------
      2201 ragu 	    5000
      2202 thasu	    7000
      2203 jack 	    4000
      2203 shajee	    6000
SQL> select * from emp5;
       ENO ENAME	  SALARY
---------- ---------- ----------
      2201 ragu 	  500000
      2202 thasu	  700000
      2203 jack 	  400000
      2203 shajee	  600000

