SQL> create table supplier(sno number(10),pno number(10),color varchar2(15),quantity number(10),primary key(sno,pno));
Table created.
SQL> desc supplier;
 Name					   Null?    Type
 ----------------------------------------- -------- ----------------------------
 SNO					   NOT NULL NUMBER(10)
 PNO					   NOT NULL NUMBER(10)
 COLOR						VARCHAR2(15)
 QUANTITY					           NUMBER(10)
SQL> insert into supplier values(&sno,&pno,'&color',&quantity);
Enter value for sno: 1001
Enter value for pno: 405
Enter value for color: blue
Enter value for quantity: 100
old   1: insert into supplier values(&sno,&pno,'&color',&quantity)
new   1: insert into supplier values(1001,405,'blue',100)
1 row created.

SQL> /
Enter value for sno: 1002
Enter value for pno: 410
Enter value for color: green
Enter value for quantity: 250
old   1: insert into supplier values(&sno,&pno,'&color',&quantity)
new   1: insert into supplier values(1002,410,'green',250)
1 row created.
SQL> /
Enter value for sno: 1003
Enter value for pno: 415
Enter value for color: red
Enter value for quantity: 500
old   1: insert into supplier values(&sno,&pno,'&color',&quantity)
new   1: insert into supplier values(1003,415,'red',500)
1 row created.
SQL> /
Enter value for sno: 1004
Enter value for pno: 420
Enter value for color: yellow
Enter value for quantity: 550
