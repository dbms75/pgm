SQL> select 8 from payroll2;
	 8
----------
	 8
	 8
	 8
	 8

SQL> set serveroutput on;
SQL> declare
  2  cursor a is select * from payroll2;
  3  x payroll2%rowtype;
  4  begin
  5  open a;
  6  loop
  7  fetch a into x;
  8  exit when a%notfound;
  9  x.hra:=x.bpay*0.5;
 10  x.pf:=x.bpay*0.12;
 11  x.da:=x.bpay*0.15;
 12  x.gpay:=x.bpay+x.hra+x.da;
 13  x.npay:=x.gpay-x.pf;
 14  update payroll2 set hra=x.hra,da=x.da,pf=x.pf,gpay=x.gpay,npay=x.npay where eno=x.eno;
 15  end loop;
 16  close a;
 17  end;
 18  /

PL/SQL procedure successfully completed.

